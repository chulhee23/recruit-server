<%= form_for @form_sheet do |f| %>
  <!-- 임시로 form sheet에 대해서 edit -->
  <div class="form-group">
    <%= f.label :title, "제목" %>
    <%= f.text_field :title, class: "form-control" %>
  </div>

  <div class="form-group">
    <%= f.label :content, "내용" %>
    <%= f.text_field :content, class: "form-control" %>
  </div>

  <button class="btn btn-primary">추가하기</button>

  <%= f.submit %>  
<% end %>
