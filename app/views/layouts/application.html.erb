<!DOCTYPE html>
<html>
	<head>
		<title>RecruitServer</title>
		<%= csrf_meta_tags %>
		<%= csp_meta_tag %>
		<meta charset="UTF-8" />
		<meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
		<title>Limitless - Responsive Web Application Kit by Eugene Kopyov</title>

		<!-- Global stylesheets -->
		<link href="https://fonts.googleapis.com/css?family=Roboto:400,300,100,500,700,900" rel="stylesheet" type="text/css">
		<link href="/limitless/assets/css/icons/icomoon/styles.min.css" rel="stylesheet" type="text/css">
		<link href="/limitless/assets/css/bootstrap.min.css" rel="stylesheet" type="text/css">
		<link href="/limitless/assets/css/bootstrap_limitless.min.css" rel="stylesheet" type="text/css">
		<link href="/limitless/assets/css/layout.min.css" rel="stylesheet" type="text/css">
		<link href="/limitless/assets/css/components.min.css" rel="stylesheet" type="text/css">
		<link href="/limitless/assets/css/colors.min.css" rel="stylesheet" type="text/css">
		<!-- /global stylesheets -->

		<!-- Core JS files -->
		<script src="/limitless/assets/js/main/jquery.min.js"></script>
		<script src="/limitless/assets/js/main/bootstrap.bundle.min.js"></script>
		<script src="/limitless/assets/js/plugins/loaders/blockui.min.js"></script>
		<script src="/limitless/assets/js/plugins/ui/ripple.min.js"></script>
		<!-- /core JS files -->

		<!-- Theme JS files -->
		<script src="/limitless/assets/js/plugins/visualization/d3/d3.min.js"></script>
		<script src="/limitless/assets/js/plugins/visualization/d3/d3_tooltip.js"></script>
		<script src="/limitless/assets/js/plugins/forms/styling/switchery.min.js"></script>
		<script src="/limitless/assets/js/plugins/forms/selects/bootstrap_multiselect.js"></script>
		<script src="/limitless/assets/js/plugins/ui/moment/moment.min.js"></script>
		<script src="/limitless/assets/js/plugins/pickers/daterangepicker.js"></script>
				<!-- Theme JS files -->
		<script src="/limitless/assets/js/plugins/extensions/jquery_ui/widgets.min.js"></script>
		<script src="/limitless/assets/js/plugins/extensions/jquery_ui/effects.min.js"></script>

		<script src="/limitless/assets/js/app.js"></script>
		<script src="/limitless/assets/js/demo_pages/dashboard.js"></script>
		<script src="/limitless/assets/js/demo_pages/jqueryui_components.js"></script>
			
		<!-- Theme JS files -->
		<script src="/limitless/assets/js/plugins/extensions/jquery_ui/interactions.min.js"></script>
		<script src="/limitless/assets/js/plugins/extensions/jquery_ui/touch.min.js"></script>

		
		<script src="/limitless/assets/js/demo_pages/jqueryui_interactions.js"></script>
		<!-- /theme JS files -->
		<script src="/limitless/assets/js/demo_pages/sidebar_secondary.js"></script>
		
		
		<%= stylesheet_link_tag    'application', media: 'all' %>
		<%= javascript_include_tag 'application' %>
	</head>
	<body class="sidebar-xs">
		<% if notice %>
			<div class="alert alert-primary">
				<%= notice %>
				<button type="button" class="close" data-dismiss="alert" aria-label="Close">
					<span aria-hidden="true">&times;</span>
				</button>
			</div>
		<% end %>
		<% if alert %>
			<div class="alert alert-danger">
				<%= alert %>
				<button type="button" class="close" data-dismiss="alert" aria-label="Close">
					<span aria-hidden="true">&times;</span>
				</button>
			</div>
		<% end %>

		<%= default_yield(:navbar) %>

		<!-- Page content -->
		<div class="page-content">

			<%= default_yield(:left_sidebar) %>

			<div class="content-wrapper">
				<%= default_yield(:header) %>
				<div class="content">
					<%= yield %>
				</div>					
				<%= default_yield(:footer) %>
				
				
				
			</div>
			
			<%= yield(:right_sidebar) %>
			
		</div>
		
	</body>

</html>
